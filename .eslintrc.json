{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
        
    ],
    "settings" : {
           "react" : {
              "version" : "latest"
           }
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off", 
        "no-mixed-spaces-and-tabs": 0  ,
        "react/prop-types": "off",
        "no-case-declarations" : "off"
    }
}
